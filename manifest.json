{
    "manifest_version": 2,
    "name": "Google Drive Launcher",
    "version": "1.0",
    "description": "A Launcher for google Drive",
    "permissions": ["*://*/*"],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["popup.js"]
        }
      ]   
}